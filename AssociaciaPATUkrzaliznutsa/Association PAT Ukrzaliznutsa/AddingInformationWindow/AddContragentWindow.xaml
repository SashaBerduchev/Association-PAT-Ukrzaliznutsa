<Window x:Class="Association_PAT_Ukrzaliznutsa.AddingInformationWindow.AddContragentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Association_PAT_Ukrzaliznutsa.AddingInformationWindow"
        mc:Ignorable="d"
        Title="AddContragentWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ComboBox Name="user" Grid.Row="0" Margin="20"/>
        <TextBox Name="NameKlient" Grid.Row="1" Margin="20"/>
        <ComboBox Name="Prodaction" Grid.Row="2" Margin="20"/>
        <ComboBox Name="VagonType" Grid.Row="3" Margin="20"/>
        <Button Name="Set" Grid.Row="4" Content="Сохранить" Margin="20" Click="Set_Click"/>
    </Grid>
</Window>
