<Window x:Class="Association_PAT_UkrzaliznutsaClient.Windows.AddProdaction"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Association_PAT_UkrzaliznutsaClient.Windows"
        mc:Ignorable="d"
        Title="AddProdaction" Height="350" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Name="ProdactionName" Grid.Row="0"  Margin="20"/>
        <TextBox Name="ProdactionCount" Grid.Row="1" Margin="20"/>
        <TextBox Name="ProdactionCost" Grid.Row="2"  Margin="20"/>
        <Button Name="Set" Grid.Row="4" Content="Сохрнаить" Margin="20" Click="Set_Click" />
    </Grid>
</Window>
